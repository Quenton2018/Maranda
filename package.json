function Vue ({ name, create }) {
  this.name = name
  this.create = create || (() => {
  })
  this.$Message(`start init: ${this.name}`)
  this.create()
  this.$Message('end init')
}

Vue.prototype.$Message = function (str) {
  console.log(this.name + ':' + str)
}

Vue.toHtml = function (c) {
  return 'html'
}

Vue.prototype.toHtml = function () {
  return Vue.toHtml(this)
}

let c1 = new Vue({
  name: 1,
  create () {
    console.log('init')
  }
})

let c2 = new Vue({
  name: 2
})
console.log(c1)
console.log(c2)

